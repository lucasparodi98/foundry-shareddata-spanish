lback: () => {\n                    resolve(\n                        'Sword01_01_Spectral_'\n                    );\n                }\n            },\n            two: {\n                label: 'Guadaña',\n                callback: () => {\n                    resolve(\n                        'Scythe01_02_Spectral_'\n                    );\n                }\n            },\n            three: {\n                label: 'Martillo',\n                callback: () => {\n                resolve(\n                        'Maul01_01_Spectral_'\n                    );\n              }\n            },\n            four: {\n                label: 'Maza',\n                callback: () => {\n                    resolve(\n                        'Mace01_01_Spectral_'\n                    );\n                }\n            },\n           }\n        }).render(true)   \n    }));\n\n    let colorArma = await (new Promise( (resolve) => {\n        new Dialog({\n        title: \"Elige color de arma:\",\n        buttons: {\n            one: {\n                label: 'Amarillo',\n                callback: () => {\n                    resolve(\n                        'Yellow'\n                    );\n                }\n            },\n            two: {\n                label: 'Azul',\n                callback: () => {\n                    resolve(\n                        'Blue'\n                    );\n                }\n            },\n            three: {\n                label: 'Morado',\n                callback: () => {\n                resolve(\n                        'Purple'\n                    );\n              }\n            },\n            four: {\n                label: 'Rojo',\n                callback: () => {\n                    resolve(\n                        'Red'\n                    );\n                }\n            },\n            five: {\n                label: 'Verde',\n                callback: () => {\n                    resolve(\n                        'Green'\n                    );\n                }\n            },\n           }\n        }).render(true)   \n    }));\n\n    let imagenToken = \"\";\n    if (tipoArma == 'Scythe01_02_Spectral_'){\n        imagenToken = \"modules/jb2a_patreon/Library/2nd_Level/Spiritual_Weapon/SpiritualWeapon_\" + tipoArma + colorArma + \"_300x300.webm\";\n    } else {\n        imagenToken = \"modules/jb2a_patreon/Library/2nd_Level/Spiritual_Weapon/SpiritualWeapon_\" + tipoArma + colorArma + \"_200x200.webm\";\n    }\n\n    let updates = {\n        token : {\n            'name':`Arma Espiritual`,\n            \"img\": imagenToken\n        },\n        actor: {\n            'name' : `Arma Espiritual`\n        },\n        embedded: { Item: {\n            \"Atacar\": {\n                \"data.attackBonus\": args[2]-8,\n                \"data.damage.parts\":[[`${damage}d8 + ${Math.floor((args[3] - 10) / 2)}`, 'force']]\n            }\n        }}\n    }\n    \n    async function myEffectFunction(template, update) {\n    //prep summoning area\n        new Sequence()\n            .effect()\n                .file(\"modules/jb2a_patreon/Library/Cantrip/Sacred_Flame/SacredFlameTarget_01_Regular_White_400x400.webm\")\n                .atLocation(template)\n                .center()\n                .scale(1)\n                .belowTokens()\n            .play()\n    }\n    \n    async function postEffects(template, token) {\n        //bring in our minion\n        new Sequence()\n            .animation()\n                .on(token)\n                    .fadeIn(500)\n            .play()\n    }\n    \n    const callbacks = {\n        pre: async (template, update) => {\n            myEffectFunction(template,update);\n            await warpgate.wait(3000);\n        },\n        post: async (template, token) => {\n        postEffects(template,token);\n        await warpgate.wait(2000);\n        }\n    };\n    \n    const options = {controllingActor: actor};\n    const summonType = \"Arma Espiritual\";\n    \n    warpgate.spawn(summonType, updates, callbacks, options);\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"enhancedcombathud":{"set1s":false,"set2s":false,"set3s":false},"autoanimations":{"killAnim":false,"below":false,"override":false,"targetToken":{"enable":false},"sourceToken":{"enable":false},"allSounds":{},"autoOverride":{"enable":false},"version":4,"audio":{"a01":{"enable":false,"file":"","delay":0,"volume":0.25},"e01":{"enable":false,"file":"","delay":0,"volume":0.25}}},"core":{"sourceId":"Compendium.SharedData.conjuros.JLvwgR1T1EFHu4J4"}},"oldValue":0},{"_id":"yfrq2SithSVtA143","name":"Inmovilizar Persona","type":"spell","img":"modules/SharedData/icons/dnd5e-legacy/spells/shielding-eerie-2.jpg","data":{"description":{"value":"<p class=\"MsoNoSpacing\">Elige a un humanoide que puedas ver dentro del alcance. El objetivo debe superar una tirada de salvación de <strong>Sabiduría</strong> o quedará @Compendium[SharedData.reglas.kg6T5RPyO9X1Dcis]{paralizado} hasta el final de la duración del conjuro. Al final de cada uno de sus turnos, el humanoide puede realizar otra tirada de salvación de Sabiduría. Si tiene éxito, el conjuro termina para ese objetivo.</p>\n<p class=\"MsoNoSpacing\"><strong>A niveles superiores. </strong>Puedes elegir como objetivo a una criatura adicional por cada nivel por encima de 2 que tenga el espacio que hayas empleado. En el momento de elegirlos como objetivo, todos los humanoides deben estar a 30 pies o menos de todos los demás.</p>","chat":"","unidentified":""},"source":"PHB pg. 251","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":1,"units":"minute"},"target":{"value":1,"width":null,"units":"","type":"creature"},"range":{"value":60,"long":0,"units":"ft"},"uses":{"value":1,"max":"1","per":"day"},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"save","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":"","value":""},"formula