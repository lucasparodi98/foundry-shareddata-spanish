ltValue":true},"quickOther":{"value":true,"altValue":true,"context":"Rondas"},"quickFlavor":{"value":true,"altValue":true},"quickSave":{"value":true,"altValue":true}},"midi-qol":{"effectActivation":false},"midiProperties":{"nodam":false,"fulldam":false,"halfdam":false,"rollOther":false,"critOther":false,"magicdam":false,"magiceffect":false,"concentration":false,"toggleEffect":false},"itemacro":{"macro":{"data":{"_id":null,"name":"4. Rayo Encogedor","type":"script","author":"5HhGOqwTwgDzIPMq","img":"icons/svg/dice-target.svg","scope":"global","command":"//DAE Macro Execute, Effect Value = \"Macro Name\" @target \n\n/**\n * For each target, the GM will have to choose \n */\n\nconst lastArg = args[args.length - 1];\nlet tactor;\nif (lastArg.tokenId) tactor = canvas.tokens.get(lastArg.tokenId).actor;\nelse tactor = game.actors.get(lastArg.actorId);\n\nlet target = canvas.tokens.get(lastArg.tokenId);\nlet tokenId = lastArg.tokenId;\nlet originalSize = target.data.width;\nlet mwak = target.actor.data.data.bonuses.mwak.damage;\n\nif (args[0] === \"on\") {\n    new Dialog({\n        title: \"Agrandar o Reducir\",\n        buttons: {\n            one: {\n                label: \"Agrandar\",\n                callback: () => {\n                    let bonus = mwak + \"+ 1d4\";\n                    let enlarge = (originalSize + 1);\n                    tactor.update({ \"data.bonuses.mwak.damage\": bonus });\n                    canvas.scene.updateEmbeddedDocuments(\"Token\", [{\"_id\": tokenId, \"width\": enlarge, \"height\": enlarge }]);\n                    DAE.setFlag(tactor, 'enlageReduceSpell', {\n                        IdToken: tokenId,\n                        size: originalSize,\n                        ogMwak: mwak,\n                    });\n                    ChatMessage.create({ content: `${target.name} está agrandado` });\n                }\n            },\n            two: {\n                label: \"Reducir\",\n                callback: () => {\n                    let bonus = mwak + \" -1d4\";\n                    let size = originalSize;\n                    let newSize = (size > 1) ? (size - 1) : (size - 0.3);\n                    tactor.update({ \"data.bonuses.mwak.damage\": bonus });\n                    canvas.scene.updateEmbeddedDocuments(\"Token\", [{\"_id\": tokenId, \"width\": newSize, \"height\": newSize }]);\n                    DAE.setFlag(tactor, 'enlageReduceSpell', {\n                        IdToken: tokenId,\n                        size: originalSize,\n                        ogMwak: mwak,\n                    });\n                    ChatMessage.create({ content: `${target.name} está reducido` });\n                }\n            },\n        }\n    }).render(true);\n}\nif (args[0] === \"off\") {\n    let flag = DAE.getFlag(tactor, 'enlageReduceSpell');\n    tactor.update({ \"data.bonuses.mwak.damage\": flag.ogMwak });\n    canvas.scene.updateEmbeddedDocuments(\"Token\", [{\"_id\": flag.IdToken, \"width\": flag.size, \"height\": flag.size }])\n    DAE.unsetFlag(tactor, 'enlageReduceSpell');\n    ChatMessage.create({ content: `${target.name} ha vuelto a su tamaño normal` });\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"autoanimations":{"version":4,"killAnim":false,"animLevel":false,"options":{"ammo":false,"menuType":"spell","variant":"01","enableCustom":false,"repeat":null,"delay":null},"override":true,"sourceToken":{"enable":false},"targetToken":{"enable":false},"levels3d":{"type":""},"macro":{"enable":false},"animType":"range","animation":"eldritchblast","color":"lightgreen","audio":{"a01":{"enable":true,"file":"modules/SharedData/music/effects/Down1.ogg","startTime":null,"volume":0.25,"delay":1500}},"preview":true,"explosions":{"enable":false}}},"newEffect":"new","changes":[]}],"effects":[],"folder":null,"sort":0,"permission":{"default":0,"5HhGOqwTwgDzIPMq":3},"flags":{"srd5e":{"page":"bestiary.html","source":"CCodex","hash":"pillar%20of%20the%20lost%20magocracy_ccodex"},"core":{"