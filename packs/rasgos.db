ll,"scale":null,"opacity":null,"unbindAlpha":false,"unbindVisibility":false,"persistent":false},"levels3d":{"type":""},"macro":{"enable":false},"animType":"static","animation":"generichealing","color":"tealyellow","preview":true,"explosions":{"enable":false}},"itemacro":{"macro":{"data":{"_id":null,"name":"Sanador Bendito","type":"script","author":"9u8C1ptPUkApyidB","img":"icons/svg/dice-target.svg","scope":"global","command":"const version = \"0.9.53\";\ntry {\n    if (args[0].itemData.type !== \"spell\") return;\n    // no healing done?\n    if (!(args[0].damageList.some(li => li.oldHP < li.newHP))) return;\n    // only targets self?\n    if (!(args[0].hitTargetUuids.some(uuid => uuid !== args[0].tokenUuid))) return;\n    // await (new Promise(resolve => setTimeout(resolve, 100)))\n    \n    \n    const tactor = await fromUuid(args[0].actorUuid);\n    const spellLevel = args[0].spellLevel;\n    const currentHP = tactor.data.data.attributes.hp.value;\n    const newHP = Math.min(tactor.data.data.attributes.hp.max, tactor.data.data.attributes.hp.value + 2 + spellLevel);\n    ChatMessage.create({content: `${tactor.name} recupera ${newHP - currentHP} PG como curación adicional`})\n    return tactor.update({\"data.attributes.hp.value\": newHP});\n} catch (err) {\n    console.error(`${args[0].itemData.name} - Sanador Bendito ${version}`, err);\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}}}}
