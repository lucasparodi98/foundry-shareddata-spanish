ro Execute, Effect Value = \"Macro Name\" @target \n\n/**\n * For each target, the GM will have to choose \n */\n\nconst lastArg = args[args.length - 1];\nlet tactor;\nif (lastArg.tokenId) tactor = canvas.tokens.get(lastArg.tokenId).actor;\nelse tactor = game.actors.get(lastArg.actorId);\n\nlet target = canvas.tokens.get(lastArg.tokenId);\nlet tokenId = lastArg.tokenId;\nlet originalSize = target.data.width;\n\nif (args[0] === \"on\") {\n    let enlarge = (originalSize + 1);\n    canvas.scene.updateEmbeddedDocuments(\"Token\", [{\"_id\": tokenId, \"width\": enlarge, \"height\": enlarge }])\n    DAE.setFlag(tactor, 'poderGigante', {\n        IdToken: tokenId,\n        size: originalSize,\n    });\n    ChatMessage.create({ content: `${target.name} está agrandado` });\n}\n            \nif (args[0] === \"off\") {\n    let flag = DAE.getFlag(tactor, 'poderGigante');\n    canvas.scene.updateEmbeddedDocuments(\"Token\", [{\"_id\": flag.IdToken, \"width\": flag.size, \"height\": flag.size }])\n    DAE.unsetFlag(tactor, 'poderGigante');\n    ChatMessage.create({ content: `${target.name} ha vuelto a su tamaño normal` });\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"core":{"sourceId":"Item.Pt7ENEMGV4ni9UnL"},"autoanimations":{"killAnim":false,"animLevel":false,"options":{"ammo":false,"scaleToToken":true,"enableCustomExplosion":false,"variant":"shockwave","scale":1},"override":true,"allSounds":{"item":{"enableAudio":true,"file":"modules/SharedData/music/effects/Monster1.ogg","delay":500,"volume":0.25}},"sourceToken":{"enable":false},"targetToken":{"enable":false},"animType":"t10","explodeVariant":"tollthedead","explodeColor":"blue","explodeRadius":5,"explodeLoop":1,"explodeDelay":0},"cf":{"id":"temp_y1xqqhgnlvg","path":"Caballero Rúnico","color":"#000000"}}}
