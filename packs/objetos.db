inter;\n      }\n          \n      /* CHECKED STYLES */\n      .venenoBasico [type=radio]:checked + img {\n        outline: 2px solid #f00;\n      }\n    </style>\n    <form class=\"venenoBasico\">\n      <div class=\"form-group\" id=\"weapons\">\n          ${weapon_content}\n      </div>\n    </form>\n    `;\n\n    new Dialog({\n        content,\n        buttons:\n        {\n            Ok:\n            {\n                label: `Ok`,\n                callback: (html) => {\n                    let itemId = $(\"input[type='radio'][name='weapon']:checked\").val();\n                    let weaponItem = tactor.items.get(itemId);\n                    let copy_item = duplicate(weaponItem);\n                    let wpDamage = copy_item.system.damage.parts;\n                    let wpSaveA = copy_item.system.save.ability;\n                    let wpSaveDC = copy_item.system.save.dc;\n                    let wpSaveS = copy_item.system.save.scaling;\n                    DAE.setFlag(tactor, `venenoBasico`, {\n                        weapon: itemId,\n                        weaponDmg: wpDamage,\n                        weaponA: wpSaveA,\n                        weaponDC: wpSaveDC,\n                        weaponS: wpSaveS,\n                    }\n                    );\n                    copy_item.system.damage.parts.push([\"1d4\", \"poison\"]);\n                    copy_item.system.save.ability = \"con\";\n                    copy_item.system.save.dc = 10;\n                    copy_item.system.save.scaling = \"flat\";\n                    tactor.updateEmbeddedDocuments(\"Item\", [copy_item]);\n                }\n            },\n            Cancel:\n            {\n                label: `Cancel`\n            }\n        }\n    }).render(true);\n}\n\n//Revert weapon and unset flag.\nif (args[0] === \"off\") {\n    let { weapon, weaponDmg, weaponA, weaponDC, weaponS, weaponColor } = DAE.getFlag(tactor, 'venenoBasico');\n    let item = tactor.items.get(weapon);\n    let copy_item = duplicate(item);\n    weaponDmg.pop();\n    copy_item.system.damage.parts = weaponDmg;\n    if (typeof weaponA == 'undefined') copy_item.system.save.ability = \"\"; else copy_item.system.save.ability = weaponA;\n    if (typeof weaponDC == 'undefined') copy_item.system.save.dc = \"\"; else copy_item.system.save.dc = weaponDC;\n    if (typeof weaponS == 'undefined') copy_item.system.save.scaling = \"spell\"; else copy_item.system.save.scaling = weaponS;\n    tactor.updateEmbeddedDocuments(\"Item\", [copy_item]);\n    DAE.unsetFlag(tactor, `venenoBasico`);\n}","author":"9u8C1ptPUkApyidB","_id":null,"img":"icons/svg/dice-target.svg","folder":null,"sort":0,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}},"enhancedcombathud":{"set1p":false,"set2p":false,"set3p":false}},"system":{"description":{"value":"<p>Puedes usar el veneno contenido en este vial para untar un arma cortante o perforante o hasta tres unidades de munición. Para hacer esto deberás invertir una acción. Cualquier criatura impactada por el arma o munición envenenadas deberá superar una tirada de salvación de <strong>Constitución CD 10</strong> o sufrirá <span style=\"color:green\">1d4 de daño de veneno.</span> Una vez aplicado al arma o munición, el veneno mantendrá su efectividad durante 1 minuto, tras el cual se habrá secado.</p>","chat":"","unidentified":""},"source":"PHB pg. 153","quantity":1,"weight":0,"price":100,"attunement":0,"equipped":false,"rarity":"common","identified":true,"activation":{"type":"action","cost":1,"condition":"un arma cortante o perforante, o hasta tres municiones"},"duration":{"value":1,"units":"minute"},"target":{"value":1,"width":null,"units":"","type":"object"},"range":{"value":null,"long":null,"units":"touch"},"uses":{"value":1,"max":"1","per":"charges","recovery":"","autoDestroy":true,"autoUse":true},"consume":{"type":"","target":"","amount":null},"ability