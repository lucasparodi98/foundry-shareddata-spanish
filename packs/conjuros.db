,"value":false,"altValue":false}},"cf":{"id":"temp_ug44f7s2mn","path":"1er Nivel","color":"#00613c"},"midi-qol":{"onUseMacroName":"","effectActivation":false},"enhancedcombathud":{"set1p":false,"set2p":false,"set3p":false},"itemacro":{"macro":{"data":{"_id":null,"name":"Disfrazarse","type":"script","author":"9u8C1ptPUkApyidB","img":"icons/svg/dice-target.svg","scope":"global","command":"const lastArg = args[args.length - 1];\nlet tactor;\nif (lastArg.tokenId) tactor = canvas.tokens.get(lastArg.tokenId).actor;\nelse tactor = game.actors.get(lastArg.actorId);\n\nif (args[0] === \"on\") {\n    let applyChanges = false;\n    new Dialog({\n      title: `Disfrazar`,\n      //Agregar Opciones para más disfraces\n      content: `\n        <form>\n          <div class=\"form-group\">\n            <label>¿Quién Soy?:</label>\n            <select id=\"disfraz\" name=\"disfraz\">\n              <option value=\"original\">Original</option>\n              <option value=\"opcion1\">Fizban</option>\n            </select>\n          </div>\n        </form>\n        `,\n      buttons: {\n        yes: {\n          icon: \"<i class='fas fa-check'></i>\",\n          label: `Me Disfrazo`,\n          callback: () => applyChanges = true\n        },\n        no: {\n          icon: \"<i class='fas fa-times'></i>\",\n          label: `Cancelar`\n        },\n      },\n      default: \"yes\",\n      close: html => {\n        if (applyChanges) {\n          let token = canvas.tokens.controlled[0];\n          let disfraz = html.find('[name=\"disfraz\"]')[0].value || \"none\";\n          //Imagen Original del Token\n          //imagenA: Imagen del Actor. imangeT: Imagen del Token\n          let imagenA = \"\";\n          let imagenT = \"\";\n          DAE.setFlag(tactor, `disfrazar`, {\n                        imagenAO: token.actor.data.img,\n                        imagenTO: token.data.img\n          });\n          // Obtener Opciones de Disfraz\n          switch (disfraz) {\n            //En caso de Agregar nuevo Disfraz, copiar del case al break, cambiar opción \n            case \"opcion1\":\n              imagenA = \"art/Humanoides/Humanos/Hombre/mago/mago%20anciano.webp\";\n              imagenT = \"tokens/Humanoides/Humanos/Hombre/mago%20anciano%20T.webp\";\n              break;\n            //Imagenes originales del Actor y Token\n            case \"original\":\n            default:\n              imagenA = token.actor.data.img;\n              imagenT = token.data.img;\n              break;\n          }\n          // Update Token\n          console.log(token);\n          token.document.update({\n            img: imagenT\n          });\n          token.actor.update({\n            img: imagenA\n          });\n        }\n      }\n    }).render(true);\n}\n// Delete Spitirual Weapon and template\nif (args[0] === \"off\") {\n  let token = canvas.tokens.controlled[0];\n  let { imagenAO, imagenTO } = DAE.getFlag(tactor, 'disfrazar');\n  //Imagenes Originales al irse efecto  \n  token.document.update({\n      img: imagenTO\n  });\n  token.actor.update({\n      img: imagenAO\n  });\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"autoanimations":{"killAnim":false,"below":false,"override":true,"targetToken":{"enable":false},"sourceToken":{"enable":false},"allSounds":{"item":{"enableAudio":true,"file":"modules/SharedData/music/effects/Magic1.ogg","delay":500,"volume":0.25}},"autoOverride":{"enable":false},"version":4,"animType":"static","animation":"outpulse","color":"purplepink","explosions":{"enable":false},"options":{"staticType":"source","variant":"02","repeat":1,"delay":250,"scale":1,"enableCustom":false,"customPath":"","menuType":"generic"},"audio":{"a01":{"enable":true,"file":"modules/SharedData/music/effects/Magic1.ogg","delay":500,"volume":0.25},"e01":{"enable":false,"file":"","delay":0,"volume":0.25}}},"midiProperties":{"nodam":false,"fulldam":false,"halfdam":false,"rollOther":false,"critOther":false,"magicdam":false,"magiceffect":fa