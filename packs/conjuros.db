const targetActor = target.actor;\n        if (!targetActor) continue;\n        // create the dim light effect on the target\n        let bsEffect = new ActiveEffect({label: \"Abrumado (Castigo Abrumador)\", icon: \"icons/magic/control/debuff-energy-hold-pink.webp\", changes: [{value: 1, mode: CONST.ACTIVE_EFFECT_MODES.ADD, priority: 20, key:\"flags.midi-qol.disadvantage.skill.all\"}, {value: 1, mode: CONST.ACTIVE_EFFECT_MODES.ADD, priority: 20, key:\"flags.midi-qol.disadvantage.attack.all\"}], duration: {seconds: 60}});\n        // 60 seconds is wrong - should look for the branding smite effect and use the remaining duration - but hey\n    \n        await MidiQOL.socket().executeAsGM(\"createEffects\", {actorUuid: targetActor.uuid, effects: [bsEffect.toObject()]})\n    }\n    Hooks.once(\"midi-qol.RollComplete\", (workflow) => {\n        console.log(\"Deleting concentration\")\n        const effect = MidiQOL.getConcentrationEffect(actor);\n        if (effect) effect.delete();\n        return true;\n    })\n    return{damageRoll: `4d6[psychic]`, flavor: \"Castigo Abrumador\"}\n} catch (err) {\n    console.error(`${args[0].itemData.name} - Castigo Abrumador ${version}`, err);\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"core":{"sourceId":"Compendium.SharedData.conjuros.cpiRHbQU8tHkvf0j"},"autoanimations":{"killAnim":false,"below":false,"override":true,"targetToken":{"enable":false},"sourceToken":{"enable":false},"allSounds":{"item":{"enableAudio":true,"file":"modules/SharedData/music/effects/Powerup.ogg","delay":500,"volume":0.25}},"autoOverride":{"enable":false},"version":4,"animType":"static","animation":"a1","color":"","explosions":{"enable":false},"options":{"staticType":"source","variant":"01","repeat":1,"delay":250,"scale":1,"enableCustom":true,"customPath":"modules/jb2a_patreon/Library/2nd_Level/Divine_Smite/DivineSmite_01_Regular_PurplePink_Caster_400x400.webm","menuType":"spell","ammo":false,"opacity":null,"unbindAlpha":false,"unbindV