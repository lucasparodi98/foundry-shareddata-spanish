QfromActorUuid(args[0].actorUuid); \n    let DC = selected.data.data.attributes.spelldc;\n\n    for (let tokenUuid of args[0].hitTargetUuids) {\n        const target = await fromUuid(tokenUuid);\n        const targetActor = target.actor;\n        const flavor = `<p>${\"Tirada de Salvación de Fuerza\"}</p><p>${CONFIG.DND5E.abilities[\"str\"]} CD ${DC} </P> `;\n        let conSave = (await targetActor.rollAbilitySave(\"str\", { flavor, fastForward: false})).total; \n     \n        if (conSave < DC) {  \n            const effectData = {\n                changes: [{key: \"macro.CUB\", mode: 0, value: \"Derribado\", priority: 20}],\n            }\n            await targetActor.createEmbeddedDocuments(\"ActiveEffect\", [effectData]);\n            await ChatMessage.create({content: `${target.name} es empujado 10 pies por ${selected.name} y está derribado!`});\n        }\n    }\n\n    Hooks.once(\"midi-qol.RollComplete\", (workflow) => {\n        console.log(\"Deleting concentration\")\n        const effect = MidiQOL.getConcentrationEffect(actor);\n        if (effect) effect.delete();\n        return true;\n    })\n    return{damageRoll: `2d6[thunder]`, flavor: \"Castigo Atronador\"}\n} catch (err) {\n    console.error(`${args[0].itemData.name} - Castigo Atronador ${version}`, err);\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"enhancedcombathud":{"set1s":false,"set2s":false,"set3s":false},"autoanimations":{"killAnim":false,"below":false,"override":true,"targetToken":{"enable":false},"sourceToken":{"enable":false},"allSounds":{"item":{"enableAudio":true,"file":"modules/SharedData/music/effects/Powerup.ogg","delay":500,"volume":0.25}},"autoOverride":{"enable":false},"version":4,"animType":"static","animation":"a1","color":"","explosions":{"enable":false},"options":{"staticType":"source","variant":"01","repeat":1,"delay":250,"scale":1,"enableCustom":true,"customPath":"modules/jb2a_patreon/Library/2nd_Level/Divine_Smite/DivineSmite_01_Regular_BlueYellow_